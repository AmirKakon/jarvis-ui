# Jarvis UI Backend Configuration
# Rename this file to .env and update with your values

# Database - PostgreSQL with asyncpg driver
# For remote server: use kakischer.tplinkdns.com:20004
DATABASE_URL=postgresql+asyncpg://n8n:n8npass@kakischer.tplinkdns.com:20004/jarvis

# n8n Webhook - Uses nginx reverse proxy with streaming support
N8N_WEBHOOK_URL=http://kakischer.tplinkdns.com:20001/n8n/webhook/365ec30d-79a8-4c1e-839b-26df01b74dfe
N8N_TIMEOUT_SECONDS=120

# Server - 0.0.0.0 allows connections from any IP
HOST=0.0.0.0
PORT=20005

# CORS - Allow all origins (use specific domains for production)
CORS_ORIGINS=*

# Session - Days before inactive sessions expire
SESSION_TTL_DAYS=30

