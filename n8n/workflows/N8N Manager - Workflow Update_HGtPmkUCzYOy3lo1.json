      "type": "n8n-nodes-base.executeWorkflowTrigger",
      "typeVersion": 1.1,
      "position": [
        0,
        0
      ],
      "id": "wfupdate-trigger-001",
      "name": "When Executed by Another Workflow"
    },
    {
      "parameters": {
        "jsCode": "// Validate and prepare workflow JSON for update\nconst input = items[0].json;\nlet workflowDef;\n\nif (!input.workflowId) {\n  throw new Error('workflowId is required');\n}\n\ntry {\n  workflowDef = typeof input.workflowJson === 'string' \n    ? JSON.parse(input.workflowJson) \n    : input.workflowJson;\n} catch (e) {\n  throw new Error('Invalid workflow JSON: ' + e.message);\n}\n\nreturn [{\n  json: {\n    workflowId: input.workflowId,\n    workflowBody: JSON.stringify(workflowDef)\n  }\n}];"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        220,
        0
      ],
      "id": "wfupdate-validate-001",
      "name": "Validate Input"
    },
    {
      "parameters": {
        "workflowId": {
          "__rl": true,
          "value": "rJoy4infFhxMynsJ",
          "mode": "id"
        },
        "workflowInputs": {
          "mappingMode": "defineBelow",
          "value": {
            "requestPath": "=/workflows/{{ $json.workflowId }}",
            "requestBody": "={{ $json.workflowBody }}",
            "requestMethod": "PUT"
          },
          "matchingColumns": [],
          "schema": [
            {
              "id": "requestPath",
              "displayName": "requestPath",
              "required": true,
