      "id": "jellyfin-trigger",
      "name": "When Executed by Another Workflow"
    },
    {
      "parameters": {
        "jsCode": "// Build Jellyfin API request based on action\nconst action = $input.first().json.action?.toLowerCase() || 'status';\nconst paramsStr = $input.first().json.params || '{}';\nlet params = {};\n\ntry {\n  params = typeof paramsStr === 'string' ? JSON.parse(paramsStr) : paramsStr;\n} catch(e) {\n  params = {};\n}\n\nconst baseUrl = 'http://localhost:20001';\nlet endpoint = '';\nlet method = 'GET';\nlet body = null;\n\nswitch(action) {\n  case 'status':\n  case 'info':\n    endpoint = '/System/Info';\n    break;\n  case 'health':\n    endpoint = '/health';\n    break;\n  case 'users':\n    endpoint = '/Users';\n    break;\n  case 'sessions':\n    endpoint = '/Sessions';\n    break;\n  case 'libraries':\n  case 'items':\n    endpoint = '/Library/VirtualFolders';\n    break;\n  case 'scan':\n  case 'refresh':\n    endpoint = '/Library/Refresh';\n    method = 'POST';\n    break;\n  case 'activity':\n    endpoint = '/System/ActivityLog/Entries';\n    break;\n  case 'scheduled-tasks':\n    endpoint = '/ScheduledTasks';\n    break;\n  case 'search':\n    const searchTerm = params.query || params.searchTerm || '';\n    endpoint = `/Items?searchTerm=${encodeURIComponent(searchTerm)}&Recursive=true&Limit=20`;\n    break;\n  case 'playing':\n    endpoint = '/Sessions?ActiveWithinSeconds=960';\n    break;\n  case 'logs':\n    endpoint = '/System/Logs';\n    break;\n  default:\n    return [{ json: { error: `Unknown action: ${action}. Valid actions: status, info, health, users, sessions, libraries, items, scan, refresh, activity, scheduled-tasks, search, playing, logs` } }];\n}\n\nreturn [{ json: { url: baseUrl + endpoint, method, body, action } }];"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        220,
        0
      ],
      "id": "jellyfin-prepare",
      "name": "Prepare Request"
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
