          "parameters": [
            {
              "name": "Accept",
              "value": "application/json"
            }
          ]
        },
        "options": {
          "timeout": 30000
        }
      },
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [
        660,
        100
      ],
      "id": "jellyfin-request",
      "name": "Jellyfin API Request"
    },
    {
      "parameters": {
        "jsCode": "// Format the Jellyfin API response\nconst result = $input.first().json;\nconst inputData = $('Prepare Request').first().json;\n\n// Truncate large responses\nlet output = result;\nif (typeof result === 'object' && Array.isArray(result) && result.length > 20) {\n  output = {\n    totalItems: result.length,\n    items: result.slice(0, 20),\n    note: `Showing first 20 of ${result.length} items`\n  };\n}\n\nreturn [{\n  json: {\n    action: inputData.action,\n    status: 'success',\n    data: output,\n    timestamp: new Date().toISOString()\n  }\n}];"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        880,
        100
      ],
      "id": "jellyfin-format",
      "name": "Format Output"
    },
    {
      "parameters": {
        "jsCode": "// Return the error\nreturn [{ json: $input.first().json }];"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        660,
        -100
      ],
